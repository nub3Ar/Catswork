{
  "manifest_version": 2,
  "name": "CatsWork_1.0",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "content_scripts" : [
    {
      "matches": ["https://mail.google.com/mail/*", "https://inbox.google.com/*"],
      "js": ["inboxsdk.js", "myapp.js"]
    }
  ],
  "page_action":{
    "default_popup": "index.html"
  }
}